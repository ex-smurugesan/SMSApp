    <default-activity>setup</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>SetupBean</managed-bean-name>
      <managed-bean-class>om.apps.sms.beans.SetupBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
